<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.adryanlist"
       name="AdryanList"
       version="2.5.1"
       provider-name="Doc Shadrach">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" optional="true" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
	<import addon="script.module.livestreamer" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>based on livestreamspro 2.3.4 by Shani</summary>
    <description>Lista de canales de Adryan.</description>
    <platform>all</platform>
  </extension>
</addon>


<addon id="repository.kodi-peripop" name="Kodi-peripop official Repo" version="1.1.0" provider-name="patipatuprimo">
	<extension point="xbmc.addon.repository" name="repository.kodi-peripop">
		<info compressed="false">https://raw.githubusercontent.com/PERIPOP/PRUEBA/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/PERIPOP/PRUEBA/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/PERIPOP/PRUEBA/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Kodi-Mexico official Repo</summary>
   		<description lang="en">Instala los Addons para Apple Tv.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
